@article{wilkinson_local_2020,
	title = {Local response in health emergencies: key considerations for addressing the {COVID}-19 pandemic in informal urban settlements},
	volume = {32},
	url = {http://dx.doi.org/10.1177/0956247820922843},
	doi = {10.1177/0956247820922843},
	abstract = {This paper highlights the major challenges and considerations for addressing COVID-19 in informal settlements. It discusses what is known about vulnerabilities and how to support local protective action. There is heightened concern about informal urban settlements because of the combination of population density and inadequate access to water and sanitation, which makes standard advice about social distancing and washing hands implausible. There are further challenges to do with the lack of reliable data and the social, political and economic contexts in each setting that will influence vulnerability and possibilities for action. The potential health impacts of COVID-19 are immense in informal settlements, but if control measures are poorly executed these could also have severe negative impacts. Public health interventions must be balanced with social and economic interventions, especially in relation to the informal economy upon which many poor urban residents depend. Local residents, leaders and community-based groups must be engaged and resourced to develop locally appropriate control strategies, in partnership with local governments and authorities. Historically, informal settlements and their residents have been stigmatized, blamed, and subjected to rules and regulations that are unaffordable or unfeasible to adhere to. Responses to COVID-19 should not repeat these mistakes. Priorities for enabling effective control measures include: collaborating with local residents who have unsurpassed knowledge of relevant spatial and social infrastructures, strengthening coordination with local governments, and investing in improved data for monitoring the response in informal settlements.},
	number = {2},
	journal = {Environment and Urbanization},
	author = {Wilkinson, Annie and Ali, Harris and Bedford, Juliet and Boonyabancha, Somsook and Connolly, Creighton and Conteh, Abu and Dean, Laura and Decorte, Filiep and Dercon, Bruno and Dias, Sonia and Dodman, David and Duijsens, Raimond and D’Urzo, Sandra and Eamer, Gwendolen and Earle, Lucy and Gupte, Jaideep and Frediani, Alex Apsan and Hasan, Arif and Hawkins, Kate and Herbst, Natalia and Kadihasanoglu, Aynur and Keil, Roger and Kibuchi, Eliud and Leach, Melissa and Lilford, Richard and Macarthy, Joseph and Mitlin, Diana and Napier, David and O’Donnell, Ian and Oyebode, Oyinlola and Ozano, Kim and Perera, Laxman and Rashid, Sabina and Ringwald, Beate and Ripoll, Santiago and Saleem, Amjad and Satterthwaite, David and Sellu, Sudie Austina and Siddique, Omar and Soesilo, Cynthia and Sommer, Kerstin and Steege, Rosie and Sverdlik, Alice and Tacoli, Cecilia and Taylor, John and Theobald, Sally and Tolhurst, Rachel and Walnycki, Anna and Watson, Samuel and Whittaker, Lana},
