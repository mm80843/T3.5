	author = {Ahmad, Aakash and Bandara, Madhushi and Fahmideh, Mahdi and Proper, Henderik A. and Guizzardi, Giancarlo and Soar, Jeffrey},
	month = oct,
	year = {2021},
	note = {ISSN: 2325-6605},
