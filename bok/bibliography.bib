@article{tze_ping_ultra_2020,
	title = {Ultra {Brief} {Psychological} {Interventions} for {COVID}-19 {Pandemic}: {Introduction} of a {Locally}-{Adapted} {Brief} {Intervention} for {Mental} {Health} and {Psychosocial} {Support} {Service}},
